# about css #

/* Reset some default styles */
body, h1, h2, p {
    margin: 0;
    padding: 0;
}

/* Basic styles for header and navigation */
header {
    background-color: #333;
    color: white;
    text-align: center;
    padding: 20px;
}

nav ul {
    list-style-type: none;
}

nav ul li {
    display: inline;
    margin-right: 20px;
}

nav a {
    color: white;
    text-decoration: none;
}

/* Styles for active link */
nav a.active {
    color: #007bff; /* Change to the same color as hover for the active link */
    border-bottom: 2px solid #007bff; /* Add an underline for the active link */
}

/* Styles for main content */
main {
    max-width: 800px;
    margin: 20px auto;
    padding: 20px;
    background-color: #f9f9f9;
    box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
}

.about, .team {
    margin-bottom: 20px;
}

.about h2, .team h2 {
    font-size: 24px;
    margin-bottom: 10px;
}

.about p, .team p {
    margin-bottom: 10px;
}

.team-member {
    text-align: center;
    margin-bottom: 20px;
}

.team-member img {
    max-width: 100%;
    border-radius: 50%;
    margin-bottom: 10px;
}

/* Footer styles */
footer {
    background-color: #333;
    color: white;
    text-align: center;
    padding: 10px;
}


